RT_DRV_DIR  := rt3352_mii
RT_DRV_PATH := ../$(RT_DRV_DIR)
RT_DRV_NAME := rt3352_iNIC

obj-$(CONFIG_RT3352_INIC_MII) += $(RT_DRV_NAME).o

$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/rt_profile.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/raconfig.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/iwhandler.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/ioctl.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/iwreq_stub.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/mbss.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/wds.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/apcli.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/comm/crc32.o
$(RT_DRV_NAME)-objs += $(RT_DRV_PATH)/mii/rt_mii_dev.o

# RT3352 MII
CONFIG_INF_TYPE = MII
CONFIG_CHIP_NAME = 3352
EXTRA_CFLAGS += -DCONFIG_INF_TYPE=INIC_INF_TYPE_MII -DCONFIG_CHIP_NAME=$(CONFIG_CHIP_NAME)
EXTRA_CFLAGS += -DRETRY_PKT_SEND

###################
#  CFLAGS
##################
EXTRA_CFLAGS += -Wall -Wstrict-prototypes -Wno-trigraphs
EXTRA_CFLAGS += -DAGGREGATION_SUPPORT -DPIGGYBACK_SUPPORT -DWMM_SUPPORT -DLINUX
EXTRA_CFLAGS += -DCONFIG_AP_SUPPORT -DAP_SCAN_SUPPORT -DUAPSD_SUPPORT -DDOT11_N_SUPPORT -DDOT1X_SUPPORT
EXTRA_CFLAGS += -DRTMP_MAC_PCI -DRTMP_RBUS_SUPPORT -DRTMP_RF_RW_SUPPORT
EXTRA_CFLAGS += -DRTMP_FLASH_SUPPORT -DCONFIG_SWMCU_SUPPORT
EXTRA_CFLAGS += -DSTATS_COUNT_SUPPORT -DENHANCED_STAT_DISPLAY -DRSSI_FEEDBACK
EXTRA_CFLAGS += -DRESOURCE_PRE_ALLOC
EXTRA_CFLAGS += -I$(RT_DRV_PATH)/comm -I$(RT_DRV_PATH)/mii

# DEBUG
EXTRA_CFLAGS += -DDBG -DFIX_POTENTIAL_BUG #-DINBAND_DEBUG

ifeq ($(CONFIG_NM_SUPPORT), y)
EXTRA_CFLAGS += -DNM_SUPPORT
endif
ifeq ($(CONFIG_MC_SUPPORT), y)
EXTRA_CFLAGS += -DMULTIPLE_CARD_SUPPORT
endif

ifeq ($(CONFIG_CONCURRENT_INIC_SUPPORT), y)
EXTRA_CFLAGS += -DCONFIG_CONCURRENT_INIC_SUPPORT
endif

ifeq ($(CONFIG_NEW_MBSS_SUPPORT), y)
EXTRA_CFLAGS += -DNEW_MBSS_SUPPORT
endif

ifeq ($(CONFIG_WOWLAN_SUPPORT), y)
EXTRA_CFLAGS += -DWOWLAN_SUPPORT
endif